#include <mem.h>
#include <gigadevice/gd32f3x0/gd32f3x0.dtsi>

/ {
	soc {
		dac: dac@40007400 {
			compatible = "gd,gd32-dac";
			reg = <0x40007400 0x400>;
			rcu-periph-clock = <0x71d>;
			num-channels = <1>;
			label = "DAC";
			status = "disabled";
			#io-channel-cells = <1>;
		};
	};
};
