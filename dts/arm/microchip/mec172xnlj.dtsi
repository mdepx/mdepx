#include "mec172xnsz.dtsi"

/ {

	soc {
		pwm9: pwm@40005890 {
			compatible = "microchip,xec-pwm";
			reg = <0x40005890 0x20>;
			pcrs = <3 31>;
			status = "disabled";
			#pwm-cells = <3>;
		};

		pwm10: pwm@400058a0 {
			compatible = "microchip,xec-pwm";
			reg = <0x400058a0 0x20>;
			pcrs = <4 0>;
			status = "disabled";
			#pwm-cells = <3>;
		};

		pwm11: pwm@400058b0 {
			compatible = "microchip,xec-pwm";
			reg = <0x400058b0 0x20>;
			pcrs = <4 1>;
			status = "disabled";
			#pwm-cells = <3>;
		};
	};
};

